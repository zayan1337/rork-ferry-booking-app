{
  "name": "expo-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "npx expo start --tunnel",
    "start-web": "npx rork start -p umeb16ahufox3btruhw4m --web --tunnel",
    "start-web-dev": "DEBUG=expo* npx rork start -p umeb16ahufox3btruhw4m --web --tunnel",
    "build:web": "npx expo export --platform web",
    "build:web:dev": "npx expo export --platform web --dev",
    "build:android:local": "cd android && ./gradlew assembleRelease",
    "build:android:debug": "cd android && ./gradlew assembleDebug",
    "build:android:eas": "eas build --platform android",
    "build:all:local": "npm run build:web && npm run build:android:local",
    "preview:web": "npx expo export --platform web --dev && npx serve dist",
    "serve:web": "npx serve dist -s",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
