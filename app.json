{
  "expo": {
    "name": "Crystal Transfer Vaavu",
    "slug": "crystal-transfer-vaavu",
    "version": "1.1.1",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "crystaltransfervaavu",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fies.crystaltransfervaavu",
      "buildNumber": "10",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "icon": "./assets/images/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fies.crystaltransfervaavu",
      "versionCode": 9,
      "permissions": ["INTERNET", "ACCESS_NETWORK_STATE", "CAMERA"]
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "experiments": {
      "typedRoutes": true
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "router": {
        "origin": "https://rork.com/"
      },
      "eas": {
        "projectId": "05b825f5-0542-410b-9f9b-5b63c4dc23ff"
      }
    },
    "owner": "fi-essystems",
    "plugins": ["expo-router", "expo-font", "expo-web-browser", "expo-updates"]
  }
}
